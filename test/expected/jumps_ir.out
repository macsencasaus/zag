; Zag Readable Intermediate Representation
; Function header formatted as:
; @func[<max-stack-size>](<param-type>,...)


@ifs[8](i32) i32:
    jmpz .L0, i32 %s[0]
    %s[4] = i32 1
    jmp .L1
.L0:
    %s[4] = i32 2
.L1:
    ret i32 %s[4]

@more_ifs[4](i32) i32:
    jmpz .L2, i32 %s[0]
    ret i32 %s[0]
    jmp .L3
.L2:
    ret i32 0
.L3:

