; Zag Readable Intermediate Representation
; Function header formatted as:
; @func[<max-stack-size>](<param-type>,...)


@main[32]() i32:
    %s[16] = ref i32 %s[0]
    %s[24] = add ptr %s[16], i64 0
    store [%s[24]], i32 1
    %s[16] = ref i32 %s[0]
    %s[24] = add ptr %s[16], i64 4
    store [%s[24]], i32 2
    %s[16] = ref i32 %s[0]
    %s[24] = add ptr %s[16], i64 8
    store [%s[24]], i32 3
    ret i32 0

@init[12]() i32:
    %s[8] = i32 1
    %s[4] = i32 2
    %s[0] = i32 3

