// RUN: %vector -emit-ir %s

fn square(x: i32): i32 {
    return x * x;
}

fn sum(a: i32, b: i32, c: i32): i32 {
    return a + b + c;
}

fn prec(): i64 {
    var x: i64 = 1 + 2 * 3;
    var y: i64 = 1 / 2 - 3;
    return x % y;
}

fn mismatch(): i32 {
    var y: i32 = 3;
    return y + 2;
}

fn prec2(): i64 {
    return 2 * 2 + 3 * 3;
}
